#############################################################################
# assembler's name
#############################################################################

AS=mac

#############################################################################
# archiver's name
#############################################################################

AR=ar

#############################################################################
# Shell command to erase file
#############################################################################

RM=del

#############################################################################
# assembler's flags
#############################################################################

SFLAGS=-fb

# archiver's flags

AFLAGS=-rs

#############################################################################

.SUFFIXES: .o .s

.s.o:
	$(AS) $(SFLAGS) $<

#############################################################################

LIBS=gpustd.a			# library names

OBJS=divhisi3.o divsi3.o mulsi3.o umulsi3.o

gpustd.a: $(OBJS)
	$(AR) $(AFLAGS) gpustd.a $(OBJS)


.PHONY:	clean realclean distclean

clean:
	$(RM) *.o *~
realclean:	clean
	$(RM) $(LIBS)
distclean:	realclean
	$(RM) *.err

$(OBJS):	libinc.s
